Shape.Julia3D fractal_shape_1 {
  SetConstant -0.3,0.68,0.08,-0.34
  SetPrecision 1e-4
  SetUVBlack 0.25
  SetUVWhite 0.6
}

Texture fractal_texture {
  LoadImage "../data/palette.png"
}

Material material_fractal {
  SetDiffuse 1,1,1
  SetReflection 0.2
  SetSpecular 0.3
  SetTexture fractal_texture
}

Transform fractal_transform {
  SetRotate y -30
  SetTranslate 0,1.5,0
}

Object fractal_1 {
  SetShape fractal_shape_1
  SetMaterial material_fractal
  SetTransform fractal_transform
}

Light.Sun sun {
  SetOrientation -2,-3.5,-2
  SetIntensity 1,1,1
}

Light.Sun sun2 {
  SetOrientation -1,0,-1
  SetIntensity 0.3,0.3,0.3
}

Light.Point bulb {
  SetPosition 2,3.5,2
  SetIntensity 2.4,5.6,8.0
}

Light.Point bulb2 {
  SetPosition 2,3.5,-2
  SetIntensity 1,0.25,0.25
}

Camera.Perspective camera_1 {
  LookAt 2,1.2,5  0,1.4,0  0,1,0
  SetFocus 1.8
}

World scene1 {
  AddCamera camera_1
  AddObject fractal_1
  AddLight sun
  AddLight sun2
  #AddLight bulb
  #AddLight bulb2
}
